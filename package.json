    "axios": "^1.12.2",
    "chart.js": "^4.5.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.19",
    "jwt-decode": "^4.0.0",
    "react": "^19.1.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.1",
    "react-icons": "^5.5.0",
    "react-intersection-observer": "^9.16.0",
    "react-router-dom": "^7.9.1",
    "tailwind-scrollbar": "^4.0.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
